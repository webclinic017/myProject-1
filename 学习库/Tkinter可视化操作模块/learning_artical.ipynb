   "source": [
    "import tkinter as tk\n",
    "\n",
    "window = tk.Tk()\n",
    "window.title('my 1st window——点击测试')\n",
    "window.geometry('400x400')\n",
    "l = tk.Label(window, text='OMG! this is Tk!', bg='green', font=('Arial', 12), width=15, height=2)\n",
    "l.pack() # 安置方式：上下左右\n",
    "window.mainloop()"
   ]
